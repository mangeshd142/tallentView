<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    
<hibernate-mapping>
	<class name="com.tallentview.contest.model.ContestWinner" table="t_contest_winners">
		<id name="winnerId">
			<generator class="increment"/>
		</id>
		<property name="goup_id" />
		<property name="user_id"/>
		<property name="contest_id"/>	
		<property name="result"/>
		<property name="complition_time"/>
	</class>
</hibernate-mapping>