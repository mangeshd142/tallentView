<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    
<hibernate-mapping>
	<class name="com.tallentview.opencontest.model.ContestInfoBean" table="t_open_contest_info">
		<id name="contest_id">
			<generator class="increment"/>
		</id>
		<property name="language"/>
		<property name="name"/>
		<property name="contest_info"/>	
		<property name="start_date"/>
		<property name="end_date"/>
		<property name="prog_id"/>
		<property name="prise1"/>
		<property name="prise2"/>
		<property name="prise3"/>
	</class>
</hibernate-mapping>