<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    
<hibernate-mapping>
	<class name="com.tallentview.contest.model.ProgramLanguage" table="t_prog_lang">
		<id name="lang_id">
			<generator class="increment"/>
		</id>
		
		<property name="lang_name"/>
		<property name="extension"/>
		<property name="Compile_cmd"/>	
		<property name="run_cmd"/>
	</class>
</hibernate-mapping>